# ----------
# Unicode & International
# ----------

# Command with unicode characters
测试
  arg?  # 测试参数
  --标志: # 测试标志
  echo "Unicode command: $arg"

# Command with emoji
emoji
  🚀?   # Rocket emoji argument
  --🎯: # Target emoji flag
  echo "Emoji command: $🚀"

# Command with special unicode
special-unicode
  arg?    # Argument with ñ, é, ü, ç characters
  --flag: # Flag with special chars
  echo "Special unicode: $arg"
